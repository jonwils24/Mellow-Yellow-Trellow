

<%= render "layouts/errors" %>


<div class="row">
  <div class="col-md-3 col-md-offset-4 landing">
    <h1>Sign In</h1>
    <form action ="/session" method="post" class="margin-base-vertical">
      <%= authenticate %>
      <label>Email<br>
        <input type="text" 
               name="user[email]" 
               placeholder="guest@gmail.com" 
               value="guest@gmail.com">
      </label><br><br>
  
      <label>Password<br>
        <input type="password"
               name="user[password]"
               placeholder="password"
               value="password">
      </label><br><br>
      <input type="submit" value="Submit" class="btn btn-primary">
    </form>
  </div>
</div>